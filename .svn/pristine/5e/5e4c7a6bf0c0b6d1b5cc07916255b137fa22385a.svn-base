body{
margin: 0;
padding: 0;
border: 0;
overflow: hidden;
height: 100%; 
max-height: 100%; 
background: #373943;
}

.fp-sidebar{
position: fixed;
top: 0;
bottom: 0; 
left: 0;
width: 280px;
height: 100%;
overflow: hidden;
background: #2a2d35;
box-sizing:border-box;
border:1px solid #24262d;
color: white;
}
.fp-hub-container{
	position:relative;
	width:100%;
	height:100%;
}
.fp-nav-tabs
{
	width: 100%;
    position: relative;
    border-bottom:1px solid #24262d;
	border-top:1px solid #24262d;
    background: #24262d;
	text-align:center;
}
.fp-nav-tabs > li
{
	width:25%;
	border-right:1px solid #1e2025;
}
.fp-nav-tabs > li:last-child
{
	border:0;
}

.fp-nav-tabs > li >  a
{
	margin:0;
	border-radius:0;
	color:#b2b2b2;
	background-color:#24262d;
	transition: all 0.3s ease;
	padding:10px 15px;
}
.fp-nav-tabs > li > a > span
{
    font-size: 20px;
}
.fp-nav-tabs > li.active >  a
{
	background:#009add;
	color:#fff;
}
.fp-nav-tabs > li > a:hover
{
	background:#009add;
	color:#fff;
}
