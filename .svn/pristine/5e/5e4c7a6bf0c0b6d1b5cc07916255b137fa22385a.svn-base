.fp-alltab-content
{
	height: 100%;
    overflow: auto;
    position: relative;
    display: block;
    width: 100%;
}
.fp-alltab-menu
{
	list-style: none;
    margin: 0;
    padding: 0;
}
.fp-alltab-menu>li {
    position: relative;
    margin: 0;
    padding: 0;
}
.fp-alltab-menu>li>a {
    padding: 12px 5px 12px 15px;
    display: block;
}
.fp-mainbar{
position: fixed;
top: 0; 
left: 280px; /*Set left value to WidthOfFrameDiv*/
right: 0;
bottom: 0;
overflow: auto; 
background: #373943;
}
.fp-logo
{
	height:60px;
	display:block;	
	overflow:hidden;
	width:100%;
	position:relative;
	background:url("../images/logo.png") no-repeat;
    background-position: center center;
    background-size: auto auto;
    padding: 0;
    margin: 0;
    text-indent: -9999px;
    transition: .3s all ease-in-out;
    -moz-transition: .3s all ease-in-out;
    -webkit-transition: .3s all ease-in-out;
}
.fp-logo a {
    display: block;
    height: 60px;
}
/*------report panel---------*/
.report-panel
{
	padding-left:10px;
	padding-right:10px;
}

.box {
    position: relative;
    border-radius: 2px !important;
	-webkit-border-radius: 2px !important;
	-moz-border-radius: 2px !important;
    background-color:#2a2d35;
    border: 1px solid #24262d;
    margin-bottom: 20px;
    width: 100%;
}
.box-header {
    color: #fff;
    display: block;
    padding: 10px;
    position: relative;
	border-bottom: 1px solid #24262d;
}
.box-header .box-title {
    display: inline-block;
    font-size: 13px;
    margin: 0;
    line-height: 1;
    text-transform: capitalize;
}
.box-header>.box-tools {
    position: absolute;
    right: 10px;
    top: 5px;
}
.box-body {
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 2px;
    border-bottom-left-radius: 2px;
    padding: 10px;
}
.box-footer {
    color: #b2b2b2;
    display: block;
	background:#26282f;
	border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 2px;
    border-bottom-left-radius: 2px;
    position: relative;
	border-top: 1px solid #24262d;
	padding:0;

}

.info-box {
    display: block;
    min-height: 64px;
    width: 100%;
}
.info-box-icon {
  /*   border-top-left-radius: 2px;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 2px; */
    display: block;
    float: left;
    height: 64px;
    width: 64px;
    text-align: center;
}
.info-box-icon img
{
	width:100%;
	height:auto;
	display:block;
}
.info-box-content {
    padding: 5px 10px;
    margin-left: 64px;
	    font-size: 13px;
    font-family: Gotham, "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-weight: normal;
    color: #b2b2b2;
    display: -webkit-box;
	line-clamp: 3;
    -webkit-line-clamp: 3;
	moz-line-clamp: 3;
    -webkit-box-orient: vertical;
    max-height: 60px;
    overflow: hidden;

}

.report-panel .box-footer ul li a {
    color: #9d9d9e;
    font-size: 12px;
    padding-left: 0;
    padding-right: 0;	
	border-radius:0;
	-moz-border-radius:0;
	/* transition: background 0.5s ease */
}
.report-panel .box-footer ul li:first-child
{
	border-right:1px solid #1e2025;
}
.report-panel .box-footer ul li a:hover, .report-panel .box-footer ul li a:focus
{
	background:	#2a2d35;	
	transition:all 0.3s ease
}
/*---------mobile -------------*/
@media (max-width: 767px)
{
	.fp-sidebar
	{
		width:100%;
	}
	.fp-mainbar
	{
		display:none;
	}	
}

