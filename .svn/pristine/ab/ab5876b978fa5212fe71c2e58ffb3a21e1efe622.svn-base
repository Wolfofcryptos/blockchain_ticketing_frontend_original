  <div class="form-horizontal">
    <div class="form-group fp-form-group">
      <label class="control-label col-sm-4" for="rpt_name">Report Name :</label>
      <div class="col-sm-8 fp-form">          
        <input type="text" class="form-control" id="rpt_name" placeholder="Enter report name" name='reportTitle' ng-model='editname'>
		<span class = "help-block"> ({{editname.length}}/25) </span>
      </div>
    </div>
	<div class="form-group fp-form-group">
      <label class="control-label col-sm-4" for="pwd">Report Url :</label>
      <div class="col-sm-8 fp-form">          
        <input type="text" class="form-control" id="pwd" placeholder="Enter password" name='reportUrl' ng-model='editurl'>
      </div>
    </div>
	<div class="form-group fp-form-group">
      <label class="control-label col-sm-4" for="pwd">Report Description :</label>
      <div class="col-sm-8 fp-form">          
		<textarea class = "form-control" id="" rows = "3" placeholder="Enter password" ng-model='editdesc'></textarea>
      </div>
    </div>
	<div class="form-group fp-form-group">
      <label class="control-label col-sm-4" for="pwd">Image Icon :</label>
		<div class="col-sm-8 fp-form">  
		  <div class="input-group">
					<label class="btn btn-upload">
							Upload&hellip; <input type="file" file-model="filename" style="display: none;">
					</label>
		  </div>
		</div>
    </div>
	<div class="form-group fp-form-group">
      <label class="control-label col-sm-4" for="pwd">Report Document Url :</label>
      <div class="col-sm-8 fp-form">          
        <input type="text" class="form-control" id="pwd" name='reportDocUrl' ng-model='editdocurl'>
      </div>
    </div>
	<div class="form-group fp-form-group">
      <label class="control-label col-sm-4" for="pwd">Report Video Url :</label>
      <div class="col-sm-8 fp-form">          
        <input type="type" class="form-control" id="pwd" placeholder="Enter password" name='reportVideoUrl' ng-model='editvideourl'>
      </div>
    </div>
  </div>
  
  
