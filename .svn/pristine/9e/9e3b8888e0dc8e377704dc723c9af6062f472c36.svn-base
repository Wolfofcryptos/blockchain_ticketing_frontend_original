.box-body {
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 2px;
    border-bottom-left-radius: 2px;
    padding: 10px;
}
.box-footer {
    color: #b2b2b2;
    display: block;
	background:#26282f;
	border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 2px;
    border-bottom-left-radius: 2px;
    position: relative;
	border-top: 1px solid #24262d;
	padding:0;

}

.info-box {
    display: block;
    min-height: 64px;
    width: 100%;
}
.info-box-icon {
  /*   border-top-left-radius: 2px;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 2px; */
    display: block;
    float: left;
    height: 64px;
    width: 64px;
    text-align: center;
}
.info-box-icon img
{
	width:100%;
	height:auto;
	display:block;
}
.info-box-content {
    padding: 5px 10px;
    margin-left: 64px;
	    font-size: 13px;
    font-family: Gotham, "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-weight: normal;
    color: #b2b2b2;
    display: -webkit-box;
	line-clamp: 3;
    -webkit-line-clamp: 3;
	moz-line-clamp: 3;
    -webkit-box-orient: vertical;
    max-height: 60px;
    overflow: hidden;

}

.fp-panel .box-footer ul li a {
    color: #9d9d9e;
    font-size: 12px;
    padding-left: 0;
    padding-right: 0;	
	border-radius:0;
	-moz-border-radius:0;
	/* transition: background 0.5s ease */
}
.fp-panel .box-footer ul li:first-child
{
	border-right:1px solid #1e2025;
}
.fp-panel .box-footer ul li a:hover, .fp-panel .box-footer ul li a:focus
{	
	color:#fff;
	background:	#2a2d35;	
	transition:all 0.3s ease
}
