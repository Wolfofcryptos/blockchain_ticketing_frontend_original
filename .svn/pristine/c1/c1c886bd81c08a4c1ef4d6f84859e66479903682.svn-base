<!-- <div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog"> -->
    <div class="readmore-popup">
      <!-- Modal content-->
      <div class="modal-content fp-modal-content">
        <div class="modal-header fp-modal-header">
          <button type="button" class="close" data-dismiss="modal" ng-click="cancel()"><span class="fp-icon icon-fp-icon-cancel"></span></button>
          <h4 class="modal-title fp-modal-title">{{title_readmore}}</h4>
        </div>
        <div class="modal-body fp-modal-body">
	<div class="fp-modal-box">
	<span class="modal-box-icon">
	<img src="{{imgurl_readmore}}">
	</span>
	<!-- .info-box-content -->
	<div class="modal-box-content">
	<span>{{description_readmore}}</span>
	</div>
	<!-- /.info-box-content -->
	</div>
        </div>
        <div class="modal-footer fp-modal-footer">
          <button type="button" class="btn btn-launch" data-dismiss="modal"><a href={{url_readmore}} target="_blank">Launch</a></button>
        </div>
      </div>
      
   </div>
  <!--</div> -->